{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": false,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ES2022",
    "target": "ES5",
    "lib": ["ES2015", "DOM"],
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "types": ["cypress"]
  }
}
